<%- include('./partials/head') -%>
<%- include('./partials/navv') -%>

<div class="container">
    <div class="row">
        <br>
        <div class="col-sm"></div>
      <div class="col-sm-8">
          <br>
          <div class="card">
            <h5 class="card-header">Cliente</h5>
            <div class="card-body">
                <form action="/clien/create" method="POST">
                    <div class="form-group row">
                      <label for="inputEmail3" class="col-sm-2 col-form-label">Nombre</label>
                      <div class="col-sm-10">
                        <input type="txt" name="nombre" class="form-control" id="" placeholder="Nombre">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="inputPassword3" class="col-sm-2 col-form-label">Apellidos</label>
                      <div class="col-sm-10">
                        <input type="txt"  name="apellidos" class="form-control" id="" placeholder="Apellidos">
                      </div>
                    </div>
                    <div class="form-group row">
                        <label for="inputPassword3" class="col-sm-2 col-form-label">Telefonp</label>
                        <div class="col-sm-10">
                          <input type="number" name="telefono" class="form-control" id="" placeholder="Telefono">
                        </div>
                      </div>
                      <div class="form-group row">
                        <label for="inputPassword3" class="col-sm-2 col-form-label">Calle</label>
                        <div class="col-sm-10">
                          <input type="txt" name="calle" class="form-control" id="" placeholder="Calle">
                        </div>
                      </div>
                      <div class="form-group row">
                        <label for="inputPassword3" class="col-sm-2 col-form-label">Numero</label>
                        <div class="col-sm-10">
                          <input type="number" name="numero" class="form-control" id="" placeholder="Numero">
                        </div>
                      </div>
                      <div class="form-group row">
                        <label for="inputPassword3" class="col-sm-2 col-form-label">Municipio</label>
                        <div class="col-sm-10">
                          <input type="text" name="municipio" class="form-control" id="" placeholder="Municipio">
                        </div>
                      </div>
                    
                   
                    <div class="form-group row">
                      <div class="col" style="text-align: center;width:100px">
                        <button type="submit" class="btn btn-primary" >Guardar</button>
                      </div>
                    </div>
                  </form>
            </div>
          </div>
       

      </div>
      <div class="col-sm"></div>
      </div>
    </div>
  </div>

<%- include('./partials/foot') -%>